<form>
	<fieldset name="gridfields">

		<field
				name="created"
				type="date"
				label="JGLOBAL_FIELD_CREATED_LABEL"
				format="LIB_GBJ_FORMAT_DATETIME_LONG" />

		<field
				name="created_by"
				type="user"
				label="JGLOBAL_FIELD_CREATED_BY_LABEL" />

		<field
				name="modified"
				type="date"
				label="JGLOBAL_FIELD_MODIFIED_LABEL"
				format="LIB_GBJ_FORMAT_DATETIME_LONG" />

		<field
				name="modified_by"
				type="user"
				label="JGLOBAL_FIELD_MODIFIED_BY_LABEL" />

		<field
				name="id"
				label="JGLOBAL_FIELD_ID_LABEL" />

	</fieldset>
