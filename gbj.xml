<extension type="library" version="3.8" method="upgrade">
    <name>LIB_GBJ</name>
    <libraryname>gbj</libraryname>
    <description>LIB_GBJ_XML_DESCRIPTION</description>
    <creationDate>February 2021</creationDate>
    <author>Libor Gabaj</author>
    <authorEmail>libor.gabaj@gmail.com</authorEmail>
    <authorUrl>https://github.com/mrkaleJoomla/lib_gbj.git</authorUrl>
    <copyright>(c) 2017-2021, Libor Gabaj</copyright>
    <license>GNU GPLv2 or later</license>
    <version>1.3.0</version>
    <packager>Libor Gabaj</packager>
    <packagerurl></packagerurl>

    <files folder="gbj">
        <folder>forms</folder>
        <folder>helpers</folder>
        <folder>layouts</folder>
        <folder>seed</folder>
        <folder>templates</folder>
        <file>LICENSE.txt</file>
        <file>html.php</file>
    </files>

    <languages folder="language">
        <language tag="en-GB">en-GB/en-GB.lib_gbj.ini</language>
        <language tag="en-GB">en-GB/en-GB.lib_gbj.sys.ini</language>
        <language tag="sk-SK">sk-SK/sk-SK.lib_gbj.ini</language>
        <language tag="sk-SK">sk-SK/sk-SK.lib_gbj.sys.ini</language>
    </languages>
